#include<bits/stdc++.h>
using namespace std;
const int mod = 1000000007;
long long  factorial(int n){
 
    
    long long fact = 1;
    for(int i=2;i<=n;i++){
      fact = (fact * i)%mod;
    }

    return fact%mod;
}
int main()
{
  long long  n;
  cin>>n;

  cout<<factorial(n)<<endl;
  return 0; 
}
